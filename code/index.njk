{# Inicio- Comunidad #}
    <div class="container">
        <div class="row py-3">
            <div class="col-12">
                <h2 class="text-center titleFont">Comunidad Tecsum</h2>
            </div>
        </div>
        <div class="card-container">
            {% for post in collections.noticias %}
            <div class="card">
                <div class="circle">
                <img src="{{ post.data.featured_image }}" class="card-img-top" alt="{{ post.data.title }}">
                </div>
                <div class="content">
                <h5 class="card-title">{{ post.data.title }}</h5>
                <p class="card-text">{{ post.data.description }}</p>
                <p class="card-text"><small class="text-muted">{{ post.data.date}}</small></p>
                <div class="container-fluid text-center">
                    <a href="{{ post.url }}" class="btn">Leer Más</a>
                </div>
                </div>
            </div>
            {% else %}
                <p>No hay publicaciones disponibles en la colección de noticias.</p>
            {% endfor %}
        </div>
    </div>
{# Final- Comunidad #}

